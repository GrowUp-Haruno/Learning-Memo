{
  "React Component set": {
    "prefix": "react-component",
    "body": [
      "import {VFC} from 'react'",
      "",
      "//Propsの型定義",
      "type PropsType = {",
      "\t",
      "}",
      "",
      "export const ${TM_FILENAME_BASE/(^.)/${1:/upcase}/}:VFC<PropsType> = (${1:props}) => {",
      "\treturn (",
      "\t\t$2",
      "\t) ",
      "}",
      ""
    ],
    "description": "Reactコンポーネントの雛形を呼び出す"
  },
  "React Custom Hooks": {
    "prefix": "react-customhooks",
    "description": "Reactカスタムフックの雛形を呼び出す",
    "body": [
      "const ${TM_FILENAME_BASE} = () => {",
      "\t$1",
      "\treturn {}",
      "};",
      "",
      "export default ${TM_FILENAME_BASE};"
    ]
  }
}
